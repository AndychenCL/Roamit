<Application
    x:Class="QuickShare.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:QuickShare"
    xmlns:common="using:QuickShare.Common"
    xmlns:rome="using:QuickShare.Rome"
    RequestedTheme="Dark">
    <Application.Resources>
        <ResourceDictionary>
            <rome:RemoteSystemKindToIconConverter x:Key="RemoteSystemKindToIconConverter"></rome:RemoteSystemKindToIconConverter>
            <rome:RemoteSystemProximityToIconConverter x:Key="RemoteSystemProximityToIconConverter"></rome:RemoteSystemProximityToIconConverter>
            <common:MathConverter x:Key="MathConverter"></common:MathConverter>
            <local:SelectedIndexToEnabledConverter x:Key="SelectedIndexToEnabledConverter"></local:SelectedIndexToEnabledConverter>
        </ResourceDictionary>
    </Application.Resources>
</Application>
